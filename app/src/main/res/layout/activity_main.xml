<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.laina.csc250_hw13_template.MainActivity">

    <LinearLayout
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="vertical"
        tools:layout_editor_absoluteY="8dp"
        tools:layout_editor_absoluteX="8dp"></LinearLayout>

    <Button
        android:id="@+id/page2Button"
        android:layout_width="253dp"
        android:layout_height="58dp"
        android:onClick="onNextPageButtonPressed"
        android:text="Goto Page 2"
        tools:layout_editor_absoluteX="66dp"
        tools:layout_editor_absoluteY="83dp" />

    <EditText
        android:id="@+id/inputET"
        android:layout_width="350dp"
        android:layout_height="60dp"
        android:ems="10"
        android:hint="The number of random numbers"
        android:inputType="number"
        tools:layout_editor_absoluteX="17dp"
        tools:layout_editor_absoluteY="8dp" />

</android.support.constraint.ConstraintLayout>
